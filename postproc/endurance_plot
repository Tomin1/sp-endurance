	next unless count_nonzero_entries($fn);
	one_plot_set(key => $graph_devgfx_count, fn => $fn, lw => 3,
	    plot => $plot, cnt => \$cnt, title => $process);
    }
    close_plot($cnt, $plot, $graph_devgfx_count);
}

